#include <iostream>
#include <stdio.h>
#include <vector>
#include <string.h>

using namespace std;

long long int a[1000];

int main()
{
    int n;
    cin>>n;

    int b=0;

    for(int i=0; i<n ; i++)
    {
        cin >> a[i];
        if(a[b]>a[i])
            b=b;
        else b=i;
    }
    cout<<b+1<<endl;
}
